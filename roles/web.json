{
  "name": "web",
  "run_list": [
    "role[base]",
    "recipe[nginx::repo]",
    "recipe[nginx]",
    "recipe[disable-nginx-default-site]",
    "recipe[server-banner]"
  ],
  "default_attributes": {
    "nginx": {
      "version": "1.5.6",
      "server_names_hash_bucket_size": 128
    },
    "firewall": {
      "rules": [
        {"http": { "port": "80" } },
        {"https": { "port": "443" } }
      ]
    }
  },
  "chef_type": "role",
  "json_class": "Chef::Role"
}
